<h1>Show List</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">List Name</th>
      <th scope="col">Created At</th>
      <th scope="col">Updated At</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">1</th>
      <td><%= @list.name %></td>
      <td><%= @list.created_at %></td>
      <td><%= @list.updated_at %></td>
    </tr>
  </tbody>
  <%= link_to 'Create Card',  new_board_list_card_path(@list.board_id,@list) %>
</table>