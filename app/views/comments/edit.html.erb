<div class = "Comment Update">
  
  <h1>Update Comment</h1>
  
  <%= form_for(@comment) do |f| %>
    <%= render(:partial => 'form', :locals => {:f => f}) %>
    
    <div class = "form-buttons">
      <%= f.submit("Update Comment")%>
    </div>
  
  <%end%>
  
  <%= link_to 'Back', comments_path %>

</div>